{
  "buildCommand": "pip install -r requirements.txt",
  "framework": "flask",
  "public": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api"
    }
  ],
  "env": {
    "FLASK_CONFIG": "production",
    "FLASK_ENV": "production"
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
